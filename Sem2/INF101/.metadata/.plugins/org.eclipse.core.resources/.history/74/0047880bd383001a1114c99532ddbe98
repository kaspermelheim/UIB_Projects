package inf101.v20.lab4.grid;

public class Grid2 {

	
	public void createBoard() {
		
		System.out.println("________");

		for(int i = 0; i < board.length; i++) {
			
			for(int j = 0; j < board[0].length; j++) {
				
				if(board[i][j] == 0) {
					System.out.print("-  ");
				}
				else {
					System.out.print(board[i][j] + "  ");
				}
			}
			System.out.println();
		}
		
		System.out.println("________");		
	}	
}
